    "@eslint/js": "9.7.0",
    "babel-plugin-inline-json-import": "0.3.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "15.8.0",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "clipboardy": "^4.0.0"
  }
