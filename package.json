{
  "type": "module",
  "name": "leetcode-randomizer",
  "version": "1.0.0",
  "description": "Gets random quetions from a leetcode study plan",
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodrigoschaer/leetcode-randomizer/"
  },
  "keywords": [
    "leetcode"
  ],
  "author": "rodrigoschaer",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/rodrigoschaer/leetcode-randomizer/issues"
  },
  "homepage": "https://github.com/rodrigoschaer/leetcode-randomizer#readme",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}'"
  },
  "devDependencies": {
    "@babel/cli": "7.24.8",
    "@babel/core": "7.24.9",
    "@babel/eslint-parser": "7.24.8",
    "@babel/plugin-syntax-json-strings": "7.8.3",
